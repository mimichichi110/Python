    "def get_min_number(numbers):\n",
    "    '''Hàm tìm giá trị nhỏ nhất'''\n",
    "    result = numbers[0]\n",
    "    for num in numbers:\n",
    "        if result > num:\n",
    "            result = num\n",
    "    return result\n",
    "\n",
    "min_number = get_min_number([10,8,34])\n",
    "print(\"Min number: \", min_number)"
   ]
  }
